    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "luxon": "^3.0.1",
    "minista": "^2.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/luxon": "^2.4.0",
    "@types/node": "18.0.6",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "8.20.0",
